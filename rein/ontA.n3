@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#>.
@prefix rein: <rein.n3#>.
@prefix ontb: <ontB.n3#>.
@prefix : <#>.

# classes and properties 
:Scientist a rdf:Class;rdfs:subClassOf :Employee.
:ResearchScientist a rdf:Class;rdfs:subClassOf :Scientist.
:PrincipalScientist a rdf:Class;rdfs:subClassOf :Scientist.
:Manager a rdf:Class;rdfs:subClassOf :Employee.
:ProjectManager a rdf:Class;rdfs:subClassOf :Manager.

:Project a rdfs:Class.

:inproject a rdf:Property;
	rdfs:domain :Project;
	rdfs:range :Employee.

:Equipment a rdfs:Class.

:Service a rdfs:Class.

# some instances

:Fax a rdfs:Class; rdfs:subClassOf :Equipment.
:QuantumNetwork a :Equipment.

:HPFax a :Fax.

:UseHPFax a :Service;
        rein:controlledBy <policyA.n3>;
	:target :HPFax.

:UseQuantumNetwork a  :Service;
        rein:controlledBy <policyA.n3>;
	:target :QuantumNetwork.

	
:QNProject a :Project;
	:inproject :Philip;
	:inproject :Matthew.

:Mark a :Manager.
:Matthew a :ProjectManager.

ontb:Alice a :Manager.

:Philip a :ResearchScientist.
:David a :PrincipalScientist.

#ends
